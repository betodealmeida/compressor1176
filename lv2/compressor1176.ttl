@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

<http://your-domain.com/plugins/compressor1176>
    a lv2:Plugin, lv2:CompressorPlugin ;
    doap:name "1176 Compressor" ;
    doap:license <http://opensource.org/licenses/GPL-3.0> ;
    lv2:port [
        a lv2:AudioPort, lv2:InputPort ;
        lv2:index 0 ;
        lv2:symbol "in" ;
        lv2:name "Input"
    ], [
        a lv2:AudioPort, lv2:OutputPort ;
        lv2:index 1 ;
        lv2:symbol "out" ;
        lv2:name "Output"
    ], [
        a lv2:ControlPort, lv2:InputPort ;
        lv2:index 2 ;
        lv2:symbol "ratio" ;
        lv2:name "Ratio" ;
        lv2:default 4.0 ;
        lv2:minimum 4.0 ;
        lv2:maximum 20.0 ;
        lv2:portProperty lv2:integer ;
        lv2:scalePoint [ rdfs:label "4:1" ; rdf:value 4.0 ] ;
        lv2:scalePoint [ rdfs:label "8:1" ; rdf:value 8.0 ] ;
        lv2:scalePoint [ rdfs:label "12:1" ; rdf:value 12.0 ] ;
        lv2:scalePoint [ rdfs:label "20:1" ; rdf:value 20.0 ] ;
    ], [
        a lv2:ControlPort, lv2:InputPort ;
        lv2:index 3 ;
        lv2:symbol "attack" ;
        lv2:name "Attack" ;
        lv2:default 800e-6 ;
        lv2:minimum 20e-6 ;
        lv2:maximum 800e-6 ;
        units:unit units:s ;
    ], [
        a lv2:ControlPort, lv2:InputPort ;
        lv2:index 4 ;
        lv2:symbol "release" ;
        lv2:name "Release" ;
        lv2:default 1.1e-1 ;
        lv2:minimum 5e-2 ;
        lv2:maximum 1.1e-1 ;
        units:unit units:s ;
    ], [
        a lv2:ControlPort, lv2:InputPort ;
        lv2:index 5 ;
        lv2:symbol "input_pad" ;
        lv2:name "Input Pad" ;
        lv2:default 0.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 24.0 ;
        units:unit units:db ;
    ] .
